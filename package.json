{
  "name": "simsim",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "brand:generate": "node scripts/generate-brand-assets.js",
    "brand:font": "node scripts/fetch-arabic-font.js",
    "brand:setup": "node scripts/fetch-arabic-font.js && node scripts/generate-brand-assets.js",
    "build:android-dev": "eas build --platform android --profile development",
    "build:android-preview": "eas build --platform android --profile preview",
    "build:android-prod": "eas build --platform android --profile production",
    "build:all": "eas build --platform all --profile production",
    "build:android-local": "cd android && gradlew.bat assembleDebug",
    "build:android-release": "cd android && gradlew.bat assembleRelease",
    "clean:android": "cd android && gradlew.bat clean",
    "install:android": "cd android && gradlew.bat installDebug",
    "postinstall": "node fix-react-native-gradle-plugin.js"
  },
  "dependencies": {
    "@expo/metro-runtime": "~4.0.1",
    "@expo/vector-icons": "~14.0.4",
    "@react-native-async-storage/async-storage": "^1.23.1",
    "@react-native/gradle-plugin": "^0.72.2",
    "@react-navigation/bottom-tabs": "*",
    "@react-navigation/drawer": "*",
    "@react-navigation/native": "*",
    "@react-navigation/native-stack": "*",
    "@react-navigation/stack": "*",
    "@supabase/supabase-js": "*",
    "canvas": "^3.1.2",
    "expo": "^52.0.0",
    "expo-constants": "~17.0.8",
    "expo-document-picker": "~13.0.3",
    "expo-image-picker": "~16.0.6",
    "expo-linear-gradient": "~14.0.2",
    "expo-location": "~18.0.10",
    "expo-status-bar": "~2.0.1",
    "lodash.isequal": "^4.5.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-native": "~0.76.0",
    "react-native-dialog": "*",
    "react-native-encrypted-storage": "*",
    "react-native-gesture-handler": "~2.20.2",
    "react-native-maps": "^1.18.0",
    "react-native-reanimated": "~3.16.1",
    "react-native-safe-area-context": "^4.12.0",
    "react-native-screens": "~4.4.0",
    "react-native-web": "^0.19.13"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@react-native-community/cli": "latest",
    "@types/react": "^18.3.23"
  },
  "private": true
}
